{
  "version": 2,
  "framework": null,
  "buildCommand": "node vercel-build.js",
  "outputDirectory": "dist-static",
  "installCommand": "npm install",
  "cleanUrls": true,
  "trailingSlash": false,
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/(favicon\\.ico|favicon\\.svg|generated-icon\\.png|og-default-image\\.svg)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=86400"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/admin-login",
      "destination": "/index.html"
    },
    {
      "source": "/admin-access",
      "destination": "/index.html"
    },
    {
      "source": "/admin-dashboard",
      "destination": "/index.html"
    },
    {
      "source": "/admin/(.*)",
      "destination": "/index.html"
    },
    {
      "source": "/set-admin-role",
      "destination": "/index.html"
    },
    {
      "source": "/dashboard",
      "destination": "/index.html"
    },
    {
      "source": "/category/(.*)",
      "destination": "/index.html"
    },
    {
      "source": "/article/(.*)",
      "destination": "/index.html"
    },
    {
      "source": "/videos/(.*)",
      "destination": "/index.html"
    },
    {
      "source": "/videos",
      "destination": "/index.html"
    },
    {
      "source": "/epaper",
      "destination": "/index.html"
    },
    {
      "source": "/search",
      "destination": "/index.html"
    },
    {
      "source": "/login",
      "destination": "/index.html"
    },
    {
      "source": "/register",
      "destination": "/index.html"
    },
    {
      "source": "/profile",
      "destination": "/index.html"
    },
    {
      "source": "/saved-articles",
      "destination": "/index.html"
    },
    {
      "source": "/reading-history",
      "destination": "/index.html"
    },
    {
      "source": "/about",
      "destination": "/index.html"
    },
    {
      "source": "/contact",
      "destination": "/index.html"
    },
    {
      "source": "/privacy-policy",
      "destination": "/index.html"
    },
    {
      "source": "/terms-of-service",
      "destination": "/index.html"
    },
    {
      "source": "/editorial-policy",
      "destination": "/index.html"
    },
    {
      "source": "/advertisement",
      "destination": "/index.html"
    },
    {
      "source": "/((?!api/|assets/|favicon|generated-icon|og-default-image|placeholder-).*)",
      "destination": "/index.html"
    }
  ]
}