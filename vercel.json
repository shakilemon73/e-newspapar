{
  "version": 2,
  "framework": null,
  "buildCommand": "node vercel-build.js",
  "outputDirectory": "dist-static",
  "installCommand": "npm install",
  "cleanUrls": true,
  "trailingSlash": false,
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/admin-login",
      "destination": "/index.html"
    },
    {
      "source": "/admin-dashboard",
      "destination": "/index.html"
    },
    {
      "source": "/admin-access",
      "destination": "/index.html"
    },
    {
      "source": "/admin/articles",
      "destination": "/index.html"
    },
    {
      "source": "/admin/categories",
      "destination": "/index.html"
    },
    {
      "source": "/admin/users",
      "destination": "/index.html"
    },
    {
      "source": "/admin/settings",
      "destination": "/index.html"
    },
    {
      "source": "/admin/videos",
      "destination": "/index.html"
    },
    {
      "source": "/admin/epapers",
      "destination": "/index.html"
    },
    {
      "source": "/admin/breaking-news",
      "destination": "/index.html"
    },
    {
      "source": "/admin/analytics",
      "destination": "/index.html"
    },
    {
      "source": "/admin/comments",
      "destination": "/index.html"
    },
    {
      "source": "/admin/audio-articles",
      "destination": "/index.html"
    },
    {
      "source": "/admin/trending",
      "destination": "/index.html"
    },
    {
      "source": "/admin/algorithms",
      "destination": "/index.html"
    },
    {
      "source": "/admin/email",
      "destination": "/index.html"
    },
    {
      "source": "/admin/advertisement",
      "destination": "/index.html"
    },
    {
      "source": "/((?!assets/|favicon|generated-icon|og-default-image|placeholder-|404).*)",
      "destination": "/index.html"
    }
  ]
}