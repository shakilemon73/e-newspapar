    },
    {
      "source": "/admin/user-dashboard",
      "destination": "/admin-user-dashboard.html"
    },
    {
      "source": "/admin/(.*)",
      "destination": "/admin-dashboard.html"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}